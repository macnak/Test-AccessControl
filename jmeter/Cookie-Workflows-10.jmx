          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.io.File

//if (props.get(&quot;productList&quot;) == null) {
    def file = new File(vars.get(&quot;products_file&quot;))
    def productList = []
    
    // Read lines and skip header
    def lines = file.readLines()
    def header = lines.remove(0) 

    lines.each { line -&gt;
        // Split by comma (handling potential whitespace)
        def cols = line.split(&quot;,&quot;).collect { it.trim() }
        if (cols.size() &gt;= 5) {
            // Store as a Map for easy access
            productList &lt;&lt; [
                id      : cols[0],
                name    : cols[1],
                category: cols[2],
                price   : cols[3],
                stock   : cols[4]
            ]
        }
    }
    props.put(&quot;productList&quot;, productList)
    log.info(&quot;Loaded &quot; + productList.size() + &quot; products.&quot;)
//}</stringProp>
