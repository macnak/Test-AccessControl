        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="JSR223 Sampler">
          <stringProp name="cacheKey">false</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.util.concurrent.ThreadLocalRandom

// 1. Get global list and shuffle for randomness
def allProds = props.get(&quot;productList&quot;).clone() // Use clone to avoid messing with global list
Collections.shuffle(allProds)

// 2. Decide how many to pick (3 to 10)
int countToPick  = ThreadLocalRandom.current().nextInt(3, 11)
def selectedProds = allProds.take(countToPick)

// 3. Decide if this user is a &quot;remover&quot; (30% chance)
boolean isRemover = Math.random() &lt; 0.3
int itemsToRemove = isRemover ? ThreadLocalRandom.current().nextInt(0, 3) : 0


// 4. Save to JMeter variables
vars.put(&quot;tg01_num_products&quot;, selectedProds.size().toString())
vars.put(&quot;tg01_items_to_remove&quot;, itemsToRemove.toString())

selectedProds.eachWithIndex { prod, idx -&gt;
    vars.put(&quot;tg01_prod_${idx}_id&quot;, prod.id)
    vars.put(&quot;tg01_prod_${idx}_name&quot;, prod.name)
    vars.put(&quot;tg01_prod_${idx}_price&quot;, prod.price)
    // Random quantity 1-3 for each
    vars.put(&quot;tg01_prod_${idx}_qty&quot;, ThreadLocalRandom.current().nextInt(1, 4).toString())
}


// Start time for pacing
vars.putObject(&quot;tg01_start_time_ms&quot;,System.currentTimeMillis())
</stringProp>
